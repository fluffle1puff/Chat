{
  "name": "ui-ui-input",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui/ui-input/src",
  "prefix": "chat-client",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui/ui-input/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/ui/ui-input/**/*.ts",
          "libs/ui/ui-input/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/ui/ui-input/.storybook",
        "browserTarget": "ui-ui-input:build-storybook",
        "compodoc": false,
        "styles": "../../../apps/chat-app/src/styles.scss",
        "stylePreprocessorOptions": {
          "includePaths": "../../../apps/chat-app/src"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/ui-ui-input",
        "configDir": "libs/ui/ui-input/.storybook",
        "browserTarget": "ui-ui-input:build-storybook",
        "compodoc": false,
        "styles": "../../../apps/chat-app/src/styles.scss",
        "stylePreprocessorOptions": {
          "includePaths": "../../../apps/chat-app/src"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
